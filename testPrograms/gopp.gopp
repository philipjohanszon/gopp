let max = fn (x, y) { return if x > y { x } else { y } }
let min = fn (x, y) { return if y < x { x } else { y } }
let abs = fn (x) { return if x > 0 { x } else { -x } }
let square = fn (x) { return x * x }
let invert = fn (x) { return -x }

let x = min(max(abs(invert(-3)), square(2)), abs(-6))

printf("%s\n", x)

